{
  "version": "1.0",
  "subjects": {
    "lab_pack_zip": {
      "require_attestation_types": ["build-provenance"],
      "allow": {
        "repository": ["${REPO}"],
        "workflow_path": [".github/workflows/release_lab_pack.yml"],
        "ref_prefix": ["refs/tags/"],
        "tag_allow_prefixes": ["v", "labpack-", "reaper-bundle-"],
        "runner_environment_allow": ["github-hosted"],
        "source_uri_allow_prefixes": ["https://github.com/${REPO}"]
      }
    },
    "verifiers": {
      "require_attestation_types": ["build-provenance"],
      "allow": {
        "repository": ["${REPO}"],
        "workflow_path": [".github/workflows/release_lab_pack.yml"],
        "ref_prefix": ["refs/tags/"],
        "tag_allow_prefixes": ["v", "labpack-", "reaper-bundle-"],
        "runner_environment_allow": ["github-hosted"],
        "source_uri_allow_prefixes": ["https://github.com/${REPO}"]
      }
    }
  },
  "strict": {
    "require_schema_validation": true,
    "deny_on_unknown_fields": false
  }
}
